{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Database/Contacts.IdentityServer.db",
    "ApplicationContextConnection": "Data Source=IdentityServer.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 4,
      "RequireDigit": false,
      "RequiredUniqueChars": 1,
      "RequireLowercase": false,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": false
    }
  }
}